<?xml version="1.0" encoding="UTF-8"?>
<!-- policy策略文件 -->
<!DOCTYPE policyconfig PUBLIC
 "-//freedesktop//DTD PolicyKit Policy Configuration 1.0//EN"
 "http://www.freedesktop.org/standards/PolicyKit/1.0/policyconfig.dtd">
<policyconfig>
  <vendor>soulxu</vendor>
  <vendor_url>www.soulxu.com</vendor_url>

  <action id="com.soulxu.test.unprivate">
    <description>soulxu test polickit</description>
    <message>soulxu test polickit</message>
    <defaults>
      <allow_inactive>yes</allow_inactive>
      <allow_active>yes</allow_active>
    </defaults>
  </action>

  <action id="com.soulxu.test.private">
    <description>soulxu test polickit</description>
    <message>soulxu test polickit</message>
    <defaults>
      <allow_inactive>no</allow_inactive>
      <allow_active>auth_admin</allow_active>
    </defaults>
  </action>


</policyconfig>
